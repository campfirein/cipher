# =====================
# Cipher Environment Example
# =====================

# --- Core Providers ---
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
OPENROUTER_API_KEY=your_openrouter_api_key

# Ollama (self-hosted, no API key needed)
OLLAMA_BASE_URL=http://localhost:11434/v1

# --- AWS Bedrock (Native) ---
# Required for native AWS Bedrock integration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# --- AWS Bedrock (OpenAI-Compatible Proxy) ---
# Use these if connecting via an OpenAI-compatible proxy
AWS_PROXY_BASE_URL=https://your-bedrock-proxy/v1
AWS_PROXY_API_KEY=your_proxy_key
AWS_PROXY_MODEL=anthropic.claude-3-sonnet-20240229-v1:0

# --- Azure OpenAI Service ---
# Required for Azure OpenAI integration
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com
AZURE_OPENAI_API_KEY=your_azure_api_key
AZURE_OPENAI_DEPLOYMENT=your_deployment_name
AZURE_OPENAI_RESOURCE=your_resource_name

# --- Optional Logging/Env ---
CIPHER_LOG_LEVEL=info
NODE_ENV=production

# --- Embedding Configuration ---
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_TIMEOUT=30000
EMBEDDING_MAX_RETRIES=3
OPENAI_ORG_ID=your_openai_org_id

# --- Storage Configuration ---
STORAGE_CACHE_TYPE=in-memory
STORAGE_CACHE_HOST=localhost
STORAGE_CACHE_PORT=6379
STORAGE_CACHE_PASSWORD=your_redis_password
STORAGE_CACHE_DATABASE=0
STORAGE_DATABASE_TYPE=in-memory
STORAGE_DATABASE_PATH=./:memory:/cipher.db
STORAGE_DATABASE_NAME=cipher

# --- Vector Store Configuration ---
VECTOR_STORE_TYPE=in-memory
VECTOR_STORE_HOST=localhost
VECTOR_STORE_PORT=6333
VECTOR_STORE_URL=http://localhost:6333
VECTOR_STORE_API_KEY=your_qdrant_api_key
VECTOR_STORE_COLLECTION=documents
VECTOR_STORE_DIMENSION=1536
VECTOR_STORE_DISTANCE=Cosine
VECTOR_STORE_ON_DISK=false
VECTOR_STORE_MAX_VECTORS=10000
VECTOR_STORE_USERNAME=your_milvus_username
VECTOR_STORE_PASSWORD=your_milvus_password